@model Password;
@{
ViewBag.Title  = "Edit";
}
<h1>Edit Password</h1>



<div class="row">
    <div class="form-horizontal col-md-6">
    @using (Html.BeginForm("Remove","Password",FormMethod.Post)) {
        @Html.HiddenFor(m=>m.Id)
        @Html.ValidationSummary(true,"",new { @class = "text-danger" })

        <div class="form-group">
            @Html.LabelFor(m => m.Id, new { @class = "control-label col-md-4" })
            <div class="form-control-static">
                @Html.DisplayFor(m => m.Id)
                @Html.HiddenFor(m => m.Id)
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Ip, new { @class = "control-label col-md-4" })
            <div class="form-control-static">@Html.DisplayFor(m => m.Ip)</div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Domain, new { @class = "control-label col-md-4" })
            <div class="col-md-8">@Html.DisplayFor(m => m.Domain)</div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Service, new { @class = "control-label col-md-4" })
            <div class="col-md-8">@Html.DisplayFor(m => m.Service)</div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.User, new { @class = "control-label col-md-4" })
            <div class="col-md-8">@Html.DisplayFor(m => m.User)</div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Passwd, new { @class = "control-label col-md-4" })
            <div class="col-md-8">@Html.DisplayFor(m => m.Passwd)</div>
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.Url, new { @class = "control-label col-md-4" })
            <div class="col-md-8"><@Html.DisplayFor(m => m.Url)</div>
        </div>
        <div class="col-md-offset-4 col-md-8">
            <input type="submit" class="btn btn-drange" value="delete" />
        </div>
    }
    </div>
</div>